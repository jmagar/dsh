# Application
NODE_ENV=development

# Server Configuration
PORT=3001
CORS_ORIGIN=http://localhost:3000

# Security
JWT_SECRET=development-jwt-secret-key-min-32-chars-long

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/dsh?schema=public

# Redis
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket
WS_PORT=3001
