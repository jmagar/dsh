    "@types/ws": "^8.5.13",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "systeminformation": "^5.21.22",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^6.0.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "prisma": "^6.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "clean": "rimraf ../dist/backend",
    "prebuild": "npm run lint && npm run test",
    "build": "tsc",
    "prestart": "npm run build",
    "dev": "ts-node -r tsconfig-paths/register --project tsconfig.json src/index.ts",
    "start": "node ../dist/backend/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix --cache --max-warnings 0",
    "prisma:generate": "prisma generate --schema=../prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "stop": "kill-port 3001"
  },
  "type": "module"
}
