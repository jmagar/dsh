# WARNING: This file contains sensitive configuration.
# Do not commit actual secrets to version control.
# Use this as a template and create a .env.local for actual secrets.

# Application
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/dsh?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"
REDIS_PASSWORD=""

# Security
JWT_SECRET=development-jwt-secret-key-min-32-chars-long

# Logging
LOG_LEVEL=debug

# Agent Configuration
AGENT_PORT=3002
AGENT_SECRET=development-agent-secret-key

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket
REACT_APP_WEBSOCKET_URL=http://localhost:3001
